num1 = float(input("Enter first number: "))
operation = input("Enter operation: ")
num2 = float(input("Enter second number: "))

if operation == '+':
  result = num1 + num2
  print(result)
elif operation == '-':
  result = num1 - num2
  print(result)
elif operation == '*':
  result = num1 * num2
  print(result)
elif operation == '/':
  if num2 != 0:
    result = num1 / num2
    print(result)
  else:
    print('error')
else:
  print('invalid operation')



